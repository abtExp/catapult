# Include all template files
include catapult/config/*.yaml
include catapult/base_images/*.Dockerfile
include catapult/compose_files/*.yaml
include catapult/templates/*.*

# Include documentation and license
include README.md
include LICENSE
include CHANGELOG.md

# Exclude development files
global-exclude __pycache__
global-exclude *.py[cod]
global-exclude *.so
global-exclude .git*
global-exclude .DS_Store